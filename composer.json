{
    "name": "tigerheck/laravel-microsoft-graph",
    "type": "library",
    "license": "MIT",
    "description": "Laravel wrapper for Microsoft Graph Rest APIs (Office365)",
    "keywords": [
        "laravel",
        "microsoft-graph",
        "msgraph",
        "graph",
        "office365",
        "o365"
    ],
    "authors": [
        {
            "name": "Chirag Goti",
            "email": "gotichirag2011@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TigerHeck\\MsGraph\\MsGraphServiceProvider"
            ],
            "aliases": {
                "MsGraph": "TigerHeck\\MsGraph\\MsGraphFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "TigerHeck\\MsGraph\\": "src"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}